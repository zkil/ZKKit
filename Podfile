platform :ios, '8.0'       #ios平台  最低版本 8.0
inhibit_all_warnings!      #不显示警告
use_frameworks!
target 'ZKKit' do
  pod 'AFNetworking', '~> 3.0'   #网络请求
  pod 'MJExtension', '~> 3.0.10'    #字典转类对象
  pod 'MJRefresh', '~> 3.1.0'    #上下拉刷新
  pod 'MBProgressHUD', '~> 1.0.0'   #活动指示器
  pod 'Masonry', '~> 1.0.1'   #约束
end

